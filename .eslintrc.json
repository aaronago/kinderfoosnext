{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "off",
        "react/react-in-jsx-scope": 0,
        "react/jsx-filename-extension": 0,
        "no-unused-vars": 1,
        "react/jsx-one-expression-per-line": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0
    }
}